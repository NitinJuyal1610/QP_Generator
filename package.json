{
  "name": "qp-generator",
  "version": "0.0.1",
  "description": "Boilerplate for typescript, postgresql and prisma. This boilerplate follows clean architecture with SOLID principles",
  "main": "app.js",
  "author": "tuancnttbk93@gmail.com",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --exit-child src/app.ts",
    "build:ts": "tsc",
    "build:clean": "rimraf ./dist",
    "build": "npm run build:clean && npm run build:ts",
    "start": "node dist/app.js"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "awilix": "9.0.0",
    "bcryptjs": "2.4.3",
    "body-parser": "^1.20.2",
    "cors": "2.8.5",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/multer": "^1.4.11",
    "prisma": "^5.6.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}
